<h1>Notes</h1>

<%= link_to 'New Note', new_note_path %>

<table>
  <thead>
    <tr>
      <th>Created</th>
      <th>Body</th>
    </tr>
  </thead>
  <tbody>
  <% @notes.each do |note| %>
    <tr>
      <td><%= link_to note.updated_at, note_path(note) %></td>
      <td><%= note.body %></td>
    </tr>
  <% end %>
  </tbody>
</table>
